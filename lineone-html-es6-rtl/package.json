    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^3.4.14"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@popperjs/core": "^2.11.8",
    "accordion-js": "^3.3.4",
    "apexcharts": "^3.54.1",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.13",
    "filepond": "^4.31.4",
    "filepond-plugin-image-preview": "^4.6.12",
    "flatpickr": "^4.6.13",
